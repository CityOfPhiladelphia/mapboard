{
  "name": "mapboard",
  "description": "A Vue.js component for data-rich web mapping",
  "version": "1.0.0",
  "author": "City of Philadelphia <maps@phila.gov>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
